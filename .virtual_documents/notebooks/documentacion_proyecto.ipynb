                  labelloc='t')
    
    # Estilo para casos
    case_style = {
        'shape': 'diamond',
        'style': 'filled',
        'fontname': 'Arial',
        'fontsize': '11'
    }
    
    # Nodos
    func_dot.node('FStart', 'INICIO\n(Recibe va)', shape='component', fillcolor='#d1e7dd', color='#0a5c36')
    func_dot.node('Case1', 'va == 1\nNo elemento', fillcolor='#f8d7da', color='#842029', **case_style)
